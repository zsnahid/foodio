#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/zahid/Documents/Personal/foodio/backend/node_modules/.pnpm/napi-postinstall@0.3.4/node_modules/napi-postinstall/lib/node_modules:/home/zahid/Documents/Personal/foodio/backend/node_modules/.pnpm/napi-postinstall@0.3.4/node_modules/napi-postinstall/node_modules:/home/zahid/Documents/Personal/foodio/backend/node_modules/.pnpm/napi-postinstall@0.3.4/node_modules:/home/zahid/Documents/Personal/foodio/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/zahid/Documents/Personal/foodio/backend/node_modules/.pnpm/napi-postinstall@0.3.4/node_modules/napi-postinstall/lib/node_modules:/home/zahid/Documents/Personal/foodio/backend/node_modules/.pnpm/napi-postinstall@0.3.4/node_modules/napi-postinstall/node_modules:/home/zahid/Documents/Personal/foodio/backend/node_modules/.pnpm/napi-postinstall@0.3.4/node_modules:/home/zahid/Documents/Personal/foodio/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli.js" "$@"
else
  exec node  "$basedir/../../lib/cli.js" "$@"
fi
