#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/zahid/Documents/Personal/foodio/backend/node_modules/.pnpm/@nuxt+opencollective@0.4.1/node_modules/@nuxt/opencollective/bin/node_modules:/home/zahid/Documents/Personal/foodio/backend/node_modules/.pnpm/@nuxt+opencollective@0.4.1/node_modules/@nuxt/opencollective/node_modules:/home/zahid/Documents/Personal/foodio/backend/node_modules/.pnpm/@nuxt+opencollective@0.4.1/node_modules/@nuxt/node_modules:/home/zahid/Documents/Personal/foodio/backend/node_modules/.pnpm/@nuxt+opencollective@0.4.1/node_modules:/home/zahid/Documents/Personal/foodio/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/zahid/Documents/Personal/foodio/backend/node_modules/.pnpm/@nuxt+opencollective@0.4.1/node_modules/@nuxt/opencollective/bin/node_modules:/home/zahid/Documents/Personal/foodio/backend/node_modules/.pnpm/@nuxt+opencollective@0.4.1/node_modules/@nuxt/opencollective/node_modules:/home/zahid/Documents/Personal/foodio/backend/node_modules/.pnpm/@nuxt+opencollective@0.4.1/node_modules/@nuxt/node_modules:/home/zahid/Documents/Personal/foodio/backend/node_modules/.pnpm/@nuxt+opencollective@0.4.1/node_modules:/home/zahid/Documents/Personal/foodio/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/opencollective.js" "$@"
else
  exec node  "$basedir/../../bin/opencollective.js" "$@"
fi
