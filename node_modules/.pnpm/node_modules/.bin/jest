#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/zahid/Documents/Personal/foodio-backend/node_modules/.pnpm/jest-cli@30.2.0_@types+node@22.19.3_ts-node@10.9.2_@types+node@22.19.3_typescript@5.9.3_/node_modules/jest-cli/bin/node_modules:/home/zahid/Documents/Personal/foodio-backend/node_modules/.pnpm/jest-cli@30.2.0_@types+node@22.19.3_ts-node@10.9.2_@types+node@22.19.3_typescript@5.9.3_/node_modules/jest-cli/node_modules:/home/zahid/Documents/Personal/foodio-backend/node_modules/.pnpm/jest-cli@30.2.0_@types+node@22.19.3_ts-node@10.9.2_@types+node@22.19.3_typescript@5.9.3_/node_modules:/home/zahid/Documents/Personal/foodio-backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/zahid/Documents/Personal/foodio-backend/node_modules/.pnpm/jest-cli@30.2.0_@types+node@22.19.3_ts-node@10.9.2_@types+node@22.19.3_typescript@5.9.3_/node_modules/jest-cli/bin/node_modules:/home/zahid/Documents/Personal/foodio-backend/node_modules/.pnpm/jest-cli@30.2.0_@types+node@22.19.3_ts-node@10.9.2_@types+node@22.19.3_typescript@5.9.3_/node_modules/jest-cli/node_modules:/home/zahid/Documents/Personal/foodio-backend/node_modules/.pnpm/jest-cli@30.2.0_@types+node@22.19.3_ts-node@10.9.2_@types+node@22.19.3_typescript@5.9.3_/node_modules:/home/zahid/Documents/Personal/foodio-backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest-cli/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest-cli/bin/jest.js" "$@"
fi
